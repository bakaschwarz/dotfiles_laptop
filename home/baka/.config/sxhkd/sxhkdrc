#
# bspwm hotkeys
#

super + Return
	urxvt

super + space
	dmenu_run

super + a
	firefox

XF86MonBrightnessUp
	xbacklight -inc 20

XF86MonBrightnessDown
	xbacklight -dec 20

XF86MyComputer
	pcmanfm

super + e
	visual-studio-code

#Wallpaper w√§hlen
super + F12
	mirage /home/baka/.wallpaper/*

# make sxhkd reload its configuration files:
super + Escape
	pkill -USR1 -x sxhkd